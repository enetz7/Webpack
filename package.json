{
  "name": "Leaflet-Webpack",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "prebuild": "rm -r dist",
    "build": "webpack --mode=production && cp -v ./src/index.html ./dist/",
    "prestart": "rm -r dist",
    "start": "webpack --mode=development",
    "poststart": "cp -v ./src/index.html ./dist/ && webpack serve"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
    "webpack-dev-server": "^3.11.0"
  }
}
